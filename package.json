{
  "name": "remote-runner",
  "displayName": "Remote Runner",
  "description": "A lightweight tool to execute build commands on remote Linux servers via SSH",
  "version": "0.0.1",
  "publisher": "galileo-studio",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:remote-runner.run",
    "onCommand:remote-runner.disconnect",
    "onCommand:remote-runner.init"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "remote-runner.run",
        "title": "Remote Runner: Run Remote Commands"
      },
      {
        "command": "remote-runner.disconnect",
        "title": "Remote Runner: Disconnect"
      },
      {
        "command": "remote-runner.init",
        "title": "$(wand) Remote Runner: Initialize Config (Wizard)"
      }
    ]
  },
  "icon": "logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Galileo927/Remote-runner.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "ssh2": "^1.14.0"
  }
}
